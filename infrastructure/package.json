{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/prepg3-stack.js"
  },
  "lib": "lib",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "rm -rf build cdk.out",
    "deploy:live": "npm run build && cdk deploy --all --context environment=live",
    "deploy:dev": "npm run build && cdk deploy --all --context environment=dev",
    "destroy": "cdk destroy --all --context environment=live",
    "diff": "cdk diff --all --context environment=live",
    "synth": "cdk synth --all --context environment=live",
    "export-config": "node ../scripts/export-aws-config.js live",
    "seed": "node ../scripts/seed-database.js",
    "create-user": "../scripts/create-test-user.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "aws-cdk": "2.1100.1",
    "esbuild": "^0.27.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "aws-amplify": "^6.15.9",
    "aws-cdk-lib": "2.232.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
